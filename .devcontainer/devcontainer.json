{
    "name": "Craft CMS",
    "image": "craftcms/php-fpm:8.1-dev",
    "postAttachCommand": [
        "php craft serve"
    ],
    "portsAttributes": {
        "9090": {
            "label": "HTTP Server",
            "onAutoForward": "openPreview"
        }
    },
    "customizations": {
        "codespaces": {
            "openFiles": []
        }
    },
    "forwardPorts": [
        8080
    ]
}
